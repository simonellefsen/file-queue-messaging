{
  "id": null,
  "uid": "rabbitmq-dashboard",
  "title": "RabbitMQ Cluster Dashboard",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "stat",
      "title": "Messages Ready",
      "id": 1,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rabbitmq_queue_messages_ready",
          "legendFormat": "ready",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Messages Unacknowledged",
      "id": 2,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rabbitmq_queue_messages_unacknowledged",
          "legendFormat": "unacked",
          "refId": "A"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Message Publish Rate",
      "id": 3,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(rabbitmq_queue_messages_published_total[1m])",
          "legendFormat": "published",
          "refId": "A"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Message Consume Rate",
      "id": 4,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(rabbitmq_queue_messages_consumed_total[1m])",
          "legendFormat": "consumed",
          "refId": "A"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Connected Consumers",
      "id": 5,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rabbitmq_consumer_count",
          "legendFormat": "consumers",
          "refId": "A"
        }
      ]
    }
  ]
}

