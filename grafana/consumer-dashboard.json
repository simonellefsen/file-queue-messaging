{
  "id": null,
  "uid": "consumer-dashboard",
  "title": "Consumer Service Dashboard",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "stat",
      "title": "Lines Written Total",
      "id": 1,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "consumer_lines_written_total",
          "legendFormat": "written",
          "refId": "A"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Lines Written Over Time",
      "id": 2,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "increase(consumer_lines_written_total[5m])",
          "legendFormat": "written",
          "refId": "A"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Message Consume Latency (p95)",
      "id": 3,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(consumer_queue_consume_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ]
    }
  ]
}

